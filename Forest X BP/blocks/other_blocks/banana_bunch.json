				}
			},
			{
				"condition": "query.block_property('test:bunch_stage') == 1",
				"components": {
					"minecraft:material_instances": {
						"*": {
							"texture": "banana_bunch1",
							"render_method": "alpha_test"
						}
					},
					"minecraft:pick_collision": {
						"origin": [
							-8,
							0,
