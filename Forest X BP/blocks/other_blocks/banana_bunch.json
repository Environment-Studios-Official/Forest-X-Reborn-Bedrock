				"set_block_property": {
					"test:bunch_stage": "(query.block_property('test:bunch_stage')<2)?query.block_property('test:bunch_stage')-2:0"
				},
				"run_command": {
					"target": "self",
					"command": [
						"playsound block.sweet_berry_bush.pick @a[r=25] ~~~ 1.0"
					]
				}
			}
